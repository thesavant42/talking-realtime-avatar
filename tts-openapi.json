{
    "components": {
        "schemas": {
            "CreateSpeechRequest": {
                "properties": {
                    "input": {
                        "description": "The text to generate audio for",
                        "maxLength": 4096,
                        "title": "Input",
                        "type": "string"
                    },
                    "instructions": {
                        "anyOf": [
                            {
                                "maxLength": 4096,
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "description": "Control the voice of your generated audio with additional instructions",
                        "title": "Instructions"
                    },
                    "model": {
                        "anyOf": [
                            {
                                "$ref": "#/components/schemas/ModelEnum"
                            },
                            {
                                "type": "string"
                            }
                        ],
                        "description": "One of the available TTS models",
                        "title": "Model"
                    },
                    "params": {
                        "anyOf": [
                            {
                                "type": "object"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "description": "Additional parameters for the TTS engine",
                        "title": "Params"
                    },
                    "response_format": {
                        "$ref": "#/components/schemas/ResponseFormatEnum",
                        "default": "wav",
                        "description": "The format to audio in"
                    },
                    "speed": {
                        "default": 1,
                        "description": "The speed of the generated audio",
                        "maximum": 4,
                        "minimum": 0.25,
                        "title": "Speed",
                        "type": "number"
                    },
                    "stream": {
                        "default": true,
                        "description": "Whether to stream the audio response",
                        "title": "Stream",
                        "type": "boolean"
                    },
                    "voice": {
                        "description": "The voice to use when generating the audio",
                        "title": "Voice",
                        "type": "string"
                    }
                },
                "required": [
                    "model",
                    "input",
                    "voice"
                ],
                "title": "CreateSpeechRequest",
                "type": "object"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "title": "Detail",
                        "type": "array"
                    }
                },
                "title": "HTTPValidationError",
                "type": "object"
            },
            "ModelEnum": {
                "enum": [
                    "hexgrad/Kokoro-82M",
                    "chatterbox",
                    "global_preset",
                    "styletts2",
                    "f5-tts",
                    "piper-tts",
                    "vall-e-x",
                    "parler-tts",
                    "megatts3",
                    "fireredtts2",
                    "higgs_v2",
                    "mms",
                    "maha_tts"
                ],
                "title": "ModelEnum",
                "type": "string"
            },
            "ResponseFormatEnum": {
                "enum": [
                    "mp3",
                    "opus",
                    "aac",
                    "flac",
                    "wav",
                    "pcm"
                ],
                "title": "ResponseFormatEnum",
                "type": "string"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        },
                        "title": "Location",
                        "type": "array"
                    },
                    "msg": {
                        "title": "Message",
                        "type": "string"
                    },
                    "type": {
                        "title": "Error Type",
                        "type": "string"
                    }
                },
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError",
                "type": "object"
            }
        }
    },
    "info": {
